{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
   "Routes": {
     "route1" : {
       "ClusterId": "modular-monolith",
       "Match": {
         "Path": "{**catch-all}"
       }
     }
   },
   "Clusters": {
     "modular-monolith": {
       "Destinations": {
         "destination1": {
           "Address": "https://localhost:7272"
         }
       }
     }
    }
  }
}
